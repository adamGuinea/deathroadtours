{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-moment": "^0.8.4",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "terser": "^3.14.1"
  },
  "proxy": "http://server:8080",
  "scripts": {
    "watch:sass": "node-sass ./src/sass/main.scss ./src/index.css -w",
    "devserver": "live-server",
    "startcss": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass ./src/sass/main.scss ./src/sass/comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 5 versions' ./src/sass/comp.css -o ./src/sass/prefix.css",
    "compress:css": "node-sass ./src/sass/prefix.css ./src/index.css -output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "concat": "^1.0.3",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2"
  }
}
